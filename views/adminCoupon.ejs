      <!-- You may want to remove or replace the above navbar with your preferred navigation. -->

      <div class="container2"> <!-- I noticed you had commented this out, you can uncomment if needed. -->
          <div class="row my-4">
              <div class="col-lg-12">
                  <div class="table-responsive">
                     

                      
                      <table class="table table-striped text-center my-3">
                        <thead>
                          <tr>
                              <th>Coupon Code</th>
                              <th>Discount Amount</th>
                              <th>Expiration Date</th>
                              <th>Minimum Purchase</th>
                              <th>Is Blocked</th>
                          </tr>
                      </thead>
                      <!-- Assuming you have a variable 'coupons' containing an array of coupon objects -->
        <tbody>
          <% coupons.forEach(coupon => { %>
              <tr>
                  <td><%= coupon.couponCode %></td>
                  <td><%= coupon.discountAmount %></td>
                  <td><%= coupon.expirationDate %></td>
                  <td><%= coupon.minimumpurchase %></td>
                  <td>
                    <% if (coupon.isBlocked) { %>
                        <!-- User is blocked, show unblock button -->
                    
            
                          <form method="post" action="/admin/couponunblock/<%= coupon._id %>">
                            <button type="submit" class="btn btn-success">Unblock</button>
                          </form>
                      <% } else { %>
                        <!-- User is not blocked, show block button -->
                        <form method="post" action="/admin/couponblock/<%= coupon._id %>">
                            <button type="submit" class="btn btn-danger">Block</button>
                          </form>
                       
                   
                      <% } %>
                      
                </td>
              </tr>
          <% }); %>
        </tbody>
        
                      </table>
