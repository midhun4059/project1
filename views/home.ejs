<script>
	document.addEventListener('DOMContentLoaded', function () {
			const triggerOverlay = document.getElementById('trigger-overlay');
			const overlay = document.querySelector('.overlay');

			// Function to toggle the overlay
			function toggleOverlay() {
					overlay.classList.toggle('open');
			}

			// Add click event listener to the trigger button
			triggerOverlay.addEventListener('click', toggleOverlay);

			// Add click event listener to the close button within the overlay
			overlay.querySelector('.overlay-close').addEventListener('click', toggleOverlay);
	});
</script>


