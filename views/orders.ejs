                                  <td style="color: black;"><%= row.stock %></td>
                                  <td>
                                      <a href="/admin/products/edit/<%= row._id %>"class="text-success"><i class="fa fa-edit fa-lg mx-1"></i></a>
                                      <a href="/admin/products/delete/<%= row._id %>" class="text-danger" onclick="checker(event)">
